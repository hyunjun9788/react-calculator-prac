{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import", "react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "endOfLine": "auto"
      }
    ],
    "react/prop-types": "off"
  }
}
